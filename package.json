  "engines": {
    "node": ">=11.10.1",
    "npm": ">=6.9.0"
  },
  "repository": "git+https://github.com/ivanbanov/cobalt-kanban.git",
  "scripts": {
    "reset": "rm -rf node_modules/ package-lock.json && npm install",
